<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/portfoliodetaillayout"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">

	<TextView android:id="@+id/portfolioHeader" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text=""
		android:textSize="20sp" android:textColor="#ff00CC33" android:textStyle="bold">
	</TextView>
	<RelativeLayout android:layout_width="wrap_content"
		android:id="@+id/relativeLayout1" android:layout_height="wrap_content">
		<TextView android:id="@+id/widget33" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Cash"
			android:textSize="15sp" android:textColor="#000000"
			android:textStyle="bold">
		</TextView>
		<TextView android:id="@+id/money1" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:textStyle="bold"
			android:textSize="15sp" android:textColor="#000000"
			android:layout_alignBaseline="@+id/widget33"
			android:layout_alignParentRight="true">
		</TextView>
	</RelativeLayout>
	<View android:layout_height="2dip" android:layout_width="fill_parent"
		android:background="#FF909090" />
	<TextView android:id="@+id/sharesLabel1" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Shares Held"
		android:textSize="15sp" android:textStyle="bold" android:textColor="#000000">
	</TextView>

	<TableLayout android:id="@+id/myTableLayout1"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:stretchColumns="*">
	</TableLayout>
</LinearLayout>